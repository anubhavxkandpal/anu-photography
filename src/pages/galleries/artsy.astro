---
import Layout from '../../layouts/Layout.astro';
import FilterableGallery from '../../components/FilterableGallery.tsx';
import artsyData from '../../data/artsy.json';
import type { GalleryImage } from '../../data/types';

const images = artsyData.images as GalleryImage[];
---

<Layout 
  title="Creative & Artsy Photography - Anu Photography"
  description={artsyData.category.description}
>
  <FilterableGallery images={images} category={artsyData.category.name} client:load />
</Layout>
