---
import Layout from '../../layouts/Layout.astro';
import FilterableGallery from '../../components/FilterableGallery.tsx';
import eventsData from '../../data/events.json';
import type { GalleryImage } from '../../data/types';

const images = eventsData.images as GalleryImage[];
---

<Layout 
  title="Event Photography - Anu Photography"
  description={eventsData.category.description}
>
  <FilterableGallery images={images} category={eventsData.category.name} client:load />
</Layout>
