---
import Layout from '../../layouts/Layout.astro';
import FilterableGallery from '../../components/FilterableGallery.tsx';
import landscapeData from '../../data/landscapes.json';
import type { GalleryImage } from '../../data/types';

const images = landscapeData.images as GalleryImage[];
---

<Layout 
  title="Nature & Landscapes Photography - Anu Photography"
  description={landscapeData.category.description}
>
  <FilterableGallery images={images} category={landscapeData.category.name} client:load />
</Layout>
